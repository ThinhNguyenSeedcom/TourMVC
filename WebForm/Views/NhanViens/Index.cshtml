@model IEnumerable<WebForm.NhanVien>

@{
    ViewBag.Title = "Index";
}

<h2>Nhân Viên</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Tên nhân viên
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenNV)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MaNV.Trim() }) |
            @Html.ActionLink("Details", "Details", new { id=item.MaNV.Trim() }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MaNV.Trim() })
        </td>
    </tr>
}

</table>
